<!DOCTYPE html>
<html lang="zxx" class="js">
<head>
    <base href="../">
    <meta charset="utf-8">
    <meta name="author" content="Softnio">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="A powerful and conceptual apps base dashboard template that especially build for developers and programmers.">
    <!-- Fav Icon  -->
    <link rel="shortcut icon" href="~/images/Icon.png">
    <!-- Page Title  -->
    <title>Urban Auto</title>
    <!-- StyleSheets  -->
    <link rel="stylesheet" href="~/Addons/assets/css/dashlite.css">
    <link id="skin-default" rel="stylesheet" href="~/Addons/assets/css/theme.css">
</head>
<body class="nk-body bg-white npc-landing ">
    <div class="nk-app-root">
        <div class="nk-main ">

            <header class="header header-32 has-header-main-s1 bg-dark" id="home">
                <div class="header-main header-main-s1 is-sticky is-transparent on-dark">
                    <div class="container header-container">
                        <div class="header-wrap">
                            <div class="header-logo">
                                <a href="/" class="logo-link">
                                    <img class="logo-light logo-img" src="~/images/Urban2x.png" alt="logo">
                                    <img class="logo-dark logo-img" src="~/images/Urban2x.png" alt="logo-dark">
                                </a>
                            </div>
                            <div class="header-toggle">
                                <button class="menu-toggler" data-target="mainNav">
                                    <em class="menu-on icon ni ni-menu"></em>
                                    <em class="menu-off icon ni ni-cross"></em>
                                </button>
                            </div><!-- .header-nav-toggle -->
                            <nav class="header-menu" data-content="mainNav">
                                <ul class="menu-list ml-lg-auto">
                                    <li class="menu-item"><a href="/" class="menu-link nav-link">Home</a></li>
                                    <li class="menu-item"><a href="#feature" class="menu-link nav-link">Features</a></li>
                                    <li class="menu-item"><a href="#pricing" class="menu-link nav-link">Pricing</a></li>
                                    <li class="menu-item"><a href="#story" class="menu-link nav-link">Story</a></li>
                                    <li class="menu-item"><a href="#" class="menu-link nav-link" data-toggle="modal" data-target="#exampleModal">Enquiry</a></li>
                                </ul>
                            </nav><!-- .nk-nav-menu -->
                        </div><!-- .header-warp-->
                    </div><!-- .container-->
                </div><!-- .header-main-->
                <div class="header-content py-6 is-dark mt-lg-n1 mt-n3">
                    <div class="container">
                        <div class="row flex-row-reverse justify-content-center text-center g-gs">
                            <div class="col-lg-6 col-md-7">
                                <div class="header-caption">
                                    <h1 class="header-title">Get Started Today with Urban Auto.</h1>
                                    <p>Urban Auto offers a comprehensive solution for managing your vehicle data efficiently. Our platform is designed to streamline your operations and enhance your business performance. </p>
                                </div><!-- .header-caption -->
                            </div><!-- .col -->
                        </div><!-- .row -->
                    </div><!-- .container -->
                </div><!-- .header-content -->
            </header><!-- .header -->

                    @RenderBody()
            <!-- Enquiry Model Start-->
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Enquiry</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form>
                                <div class="row col-md-12">
                                    <div class="form-group col-md-6">
                                        <label class="form-label" for="default-04">First Name</label>
                                        <input type="text" class="form-control" id="recipient-name" placeholder="Enter your first name">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="form-label" for="default-04">Last Name</label>
                                        <input type="text" class="form-control" id="recipient-name" placeholder="Enter your last name">
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="form-label" for="fv-phone">Phone</label>
                                            <div class="form-control-wrap">
                                                <div class="input-group">
                                                    <div class="input-group-prepend">
                                                        <span class="input-group-text" id="fv-phone">+91</span>
                                                    </div>
                                                    <input type="number" class="form-control" placeholder="Enter your number">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="form-label" for="default-04">Email</label>
                                        <div class="form-control-wrap">
                                            <div class="form-icon form-icon-right">
                                                <em class="icon ni ni-mail"></em>
                                            </div>
                                            <input type="email" class="form-control" id="default-04" placeholder="Enter your email">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="form-label">State</label>
                                        <div class="form-control-wrap">
                                            <select onchange="print_city('state', this.selectedIndex);" id="sts" name="stt" class="form-control" required></select>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="form-label">City</label>
                                        <div class="form-control-wrap">
                                            <select id="state" class="form-control" required></select>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label for="message-text" class="col-form-label text-dark">Message:</label>
                                        <textarea class="form-control" id="message-text"></textarea>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Send enquiry</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Enquiry Model End -->


            <footer class="footer bg-dark is-dark section section-sm" id="footer">
                <div class="container">
                    <div class="row g-3 align-items-center">
                        <div class="col-md-3">
                            <div class="footer-logo">
                                <a href="/" class="logo-link">
                                    <img class="logo-light logo-img" src="~/images/Urban2x.png" alt="logo">
                                    <img class="logo-dark logo-img" src="~/images/Urban2x.png" alt="logo-dark">
                                </a>
                            </div><!-- .footer-logo -->
                        </div><!-- .col -->
                        <div class="col-md-9 d-flex justify-content-md-end">
                            <ul class="link-inline gx-4">
                                <li><a href="@Url.Action("TermsandCondition", "Home")">Terms & Condition</a></li>
                            </ul><!-- .footer-nav -->
                        </div><!-- .col -->
                    </div>
                    <div class="row g-3 align-items-center pt-3">
                        <div class="col-md-8">
                            <div class="text-base">&copy; @DateTime.Now.Year . Developed by <span class="text-base fw-bold">Yash Panchal</span></div>
                        </div><!-- .col -->
                        <div class="col-md-4 d-flex justify-content-md-end">
                            <ul class="social">
                                <li><a href="tel:+919327118853"><em class="icon ni ni-call"></em></a></li>
                                <li><a href="https://www.facebook.com/profile.php?id=61558067462158" target="_blank"><em class="icon ni ni-facebook-f"></em></a></li>
                                <li><a href="mailto:UrbanAuto.Online@gmail.com" target="_blank"><em class="icon ni ni-mail"></em></a></li>
                            </ul><!-- .footer-icon -->
                        </div><!-- .col -->
                    </div><!-- .row -->
                </div><!-- .container -->
            </footer><!-- .footer -->
        </div>
    </div>
    <script src="~/js/cities.js"></script>
    <script language="javascript">print_state("sts");</script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/Addons/assets/js/bundle.js" asp-append-version="true"></script>
    <script src="~/Addons/assets/js/scripts.js" asp-append-version="true"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
